<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @notas.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">Cancel</a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @notas.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="notas[nota]" class="fieldLabel">Nota</label>
            <input type="text" id="notas[nota]" name="notas[nota]" <%= placeholder("Nota") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
