<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @horario.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">Cancel</a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @horario.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="horario[seccion]" class="fieldLabel">Seccion</label>
            <input type="text" id="horario[seccion]" name="horario[seccion]" <%= placeholder("Seccion") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="horario[dia]" class="fieldLabel">Dia</label>
            <input type="text" id="horario[dia]" name="horario[dia]" <%= placeholder("Dia") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="horario[horainicio]" class="fieldLabel">Horainicio</label>
            <input type="text" id="horario[horainicio]" name="horario[horainicio]" <%= placeholder("Horainicio") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="horario[horafin]" class="fieldLabel">Horafin</label>
            <input type="text" id="horario[horafin]" name="horario[horafin]" <%= placeholder("Horafin") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="horario[salon]" class="fieldLabel">Salon</label>
            <input type="text" id="horario[salon]" name="horario[salon]" <%= placeholder("Salon") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="horario[sede]" class="fieldLabel">Sede</label>
            <input type="text" id="horario[sede]" name="horario[sede]" <%= placeholder("Sede") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="horario[tipoclase]" class="fieldLabel">Tipoclase</label>
            <input type="text" id="horario[tipoclase]" name="horario[tipoclase]" <%= placeholder("Tipoclase") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="horario[frecuencia]" class="fieldLabel">Frecuencia</label>
            <input type="text" id="horario[frecuencia]" name="horario[frecuencia]" <%= placeholder("Frecuencia") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="horario[fecha]" class="fieldLabel">Fecha</label>
            <input type="text" id="horario[fecha]" name="horario[fecha]" <%= placeholder("Fecha") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
