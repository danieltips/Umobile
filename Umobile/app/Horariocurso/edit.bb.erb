<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @horariocurso.object %>
    <h1>Edit <%= @horariocurso.id%></h1>
    <%= link_to "Delete", :action => :delete, :id => @horariocurso.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @horariocurso.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Id: </td>
              <td class="itemValue"><input type="text" name="horariocurso[id]" value="<%= @horariocurso.id %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Idcurso: </td>
              <td class="itemValue"><input type="text" name="horariocurso[idcurso]" value="<%= @horariocurso.idcurso %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Dia: </td>
              <td class="itemValue"><input type="text" name="horariocurso[dia]" value="<%= @horariocurso.dia %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Horainicio: </td>
              <td class="itemValue"><input type="text" name="horariocurso[horainicio]" value="<%= @horariocurso.horainicio %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Horafin: </td>
              <td class="itemValue"><input type="text" name="horariocurso[horafin]" value="<%= @horariocurso.horafin %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Salon: </td>
              <td class="itemValue"><input type="text" name="horariocurso[salon]" value="<%= @horariocurso.salon %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Sede: </td>
              <td class="itemValue"><input type="text" name="horariocurso[sede]" value="<%= @horariocurso.sede %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Tipo: </td>
              <td class="itemValue"><input type="text" name="horariocurso[tipo]" value="<%= @horariocurso.tipo %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
