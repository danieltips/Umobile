<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New Horariocurso</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @horariocurso.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Id: </td>
                <td class="itemValue"><input type="text" name="horariocurso[id]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Idcurso: </td>
                <td class="itemValue"><input type="text" name="horariocurso[idcurso]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Dia: </td>
                <td class="itemValue"><input type="text" name="horariocurso[dia]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Horainicio: </td>
                <td class="itemValue"><input type="text" name="horariocurso[horainicio]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Horafin: </td>
                <td class="itemValue"><input type="text" name="horariocurso[horafin]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Salon: </td>
                <td class="itemValue"><input type="text" name="horariocurso[salon]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Sede: </td>
                <td class="itemValue"><input type="text" name="horariocurso[sede]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Tipo: </td>
                <td class="itemValue"><input type="text" name="horariocurso[tipo]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
