<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @periodo.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @periodo.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="periodo[id]" class="fieldLabel">Id</label>
            <input type="text" id="periodo[id]" name="periodo[id]" <%= placeholder("Id") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="periodo[inicio]" class="fieldLabel">Inicio</label>
            <input type="text" id="periodo[inicio]" name="periodo[inicio]" <%= placeholder("Inicio") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="periodo[fin]" class="fieldLabel">Fin</label>
            <input type="text" id="periodo[fin]" name="periodo[fin]" <%= placeholder("Fin") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="periodo[periodo]" class="fieldLabel">Periodo</label>
            <input type="text" id="periodo[periodo]" name="periodo[periodo]" <%= placeholder("Periodo") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
