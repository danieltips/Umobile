<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @promedio.id %></h1>
    <a href="<%= url_for :action => :show, :id => @promedio.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">Cancel</a>
    <a href="<%= url_for :action => :delete, :id => @promedio.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">Delete</a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @promedio.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="promedio[id]" class="fieldLabel">Id</label>
            <input type="text" id="promedio[id]" name="promedio[id]" value="<%= @promedio.id %>" <%= placeholder( "Id" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="promedio[promedio]" class="fieldLabel">Promedio</label>
            <input type="text" id="promedio[promedio]" name="promedio[promedio]" value="<%= @promedio.promedio %>" <%= placeholder( "Promedio" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
