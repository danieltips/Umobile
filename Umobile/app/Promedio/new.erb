<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @promedio.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">Cancel</a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @promedio.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="promedio[id]" class="fieldLabel">Id</label>
            <input type="text" id="promedio[id]" name="promedio[id]" <%= placeholder("Id") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="promedio[promedio]" class="fieldLabel">Promedio</label>
            <input type="text" id="promedio[promedio]" name="promedio[promedio]" <%= placeholder("Promedio") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
