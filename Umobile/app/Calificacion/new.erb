<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @calificacion.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">Cancel</a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @calificacion.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="calificacion[id]" class="fieldLabel">Id</label>
            <input type="text" id="calificacion[id]" name="calificacion[id]" <%= placeholder("Id") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="calificacion[idcurso]" class="fieldLabel">Idcurso</label>
            <input type="text" id="calificacion[idcurso]" name="calificacion[idcurso]" <%= placeholder("Idcurso") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="calificacion[evaluacion]" class="fieldLabel">Evaluacion</label>
            <input type="text" id="calificacion[evaluacion]" name="calificacion[evaluacion]" <%= placeholder("Evaluacion") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="calificacion[nota]" class="fieldLabel">Nota</label>
            <input type="text" id="calificacion[nota]" name="calificacion[nota]" <%= placeholder("Nota") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="calificacion[numero]" class="fieldLabel">Numero</label>
            <input type="text" id="calificacion[numero]" name="calificacion[numero]" <%= placeholder("Numero") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="calificacion[peso]" class="fieldLabel">Peso</label>
            <input type="text" id="calificacion[peso]" name="calificacion[peso]" <%= placeholder("Peso") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="calificacion[tipo]" class="fieldLabel">Tipo</label>
            <input type="text" id="calificacion[tipo]" name="calificacion[tipo]" <%= placeholder("Tipo") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
