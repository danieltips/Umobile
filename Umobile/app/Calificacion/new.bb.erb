<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New Calificacion</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @calificacion.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Id: </td>
                <td class="itemValue"><input type="text" name="calificacion[id]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Idcurso: </td>
                <td class="itemValue"><input type="text" name="calificacion[idcurso]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Evaluacion: </td>
                <td class="itemValue"><input type="text" name="calificacion[evaluacion]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Nota: </td>
                <td class="itemValue"><input type="text" name="calificacion[nota]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Numero: </td>
                <td class="itemValue"><input type="text" name="calificacion[numero]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Peso: </td>
                <td class="itemValue"><input type="text" name="calificacion[peso]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Tipo: </td>
                <td class="itemValue"><input type="text" name="calificacion[tipo]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
